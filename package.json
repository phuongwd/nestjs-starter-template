  "scripts": {
    "build": "nest build --webpack && node copy-templates.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --webpack --watch",
    "start:dev:https": "USE_HTTPS=true nest start --webpack --watch",
    "generate:ssl": "./scripts/generate-ssl-certs.sh",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "type-check": "sh -c 'tsc --noEmit -p tsconfig.json'",
    "lint": "eslint --fix --cache --cache-location ./node_modules/.cache/.eslintcache 'src/**/*.{js,jsx,ts,tsx}' && yarn type-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.config.js",
    "check:all": "npm run type-check && npm run lint && npm test",
    "prepare": "[ \"$NODE_ENV\" != \"production\" ] && npx husky install || exit 0"
  },
  "lint-staged": {
    "*src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache --cache-location ./node_modules/.cache/.eslintcache"
    ],
    "*src/**/*.{ts,tsx}": [
      "npm run type-check"
    ]
  },
